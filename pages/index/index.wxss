/**index.wxss**/
.container{
  font-size: 26rpx;
  overflow: hidden;
}
.management{
    display: flex;
    justify-content: space-between;
    height: 70rpx;
    line-height: 70rpx;
    padding: 10rpx 20rpx;
    margin-top: 30rpx;
}
.management .left{
  width: 70rpx;
  height: 70rpx;
  z-index: 1;
  border-radius: 50%;
  background: url(https://friend-check.playonwechat.com/static/images/stop_music.png) no-repeat;
  background-size: 100%;
}
.left.music_play{
 background: url(https://friend-check.playonwechat.com/static/images/music.png) no-repeat;
  background-size: 100%;
}
.management .right{
    background: #34a8e0;
    color: #fff;
    height: 60rpx;
    line-height: 60rpx;
    border-radius: 10rpx;
    padding: 0 20rpx;
}
.notice{
  text-align: center;
}
.buttons view{
  display: block;
  width: 200rpx;
  height: 60rpx;
  line-height: 60rpx;
  border-radius: 10rpx;
  padding: 0 20rpx;
  background: #666;
  color: #fff;
  margin: 20rpx auto;
  text-align: center;
  cursor: pointer;
}
.buttons view.active{
  background: #34a8e0;
}
.buttons button{
  font-size: 26rpx;
  margin: 0;
  padding: 0;
  border-radius: 0;
  background: #34a8e0;
  color: #fff;
  line-height: inherit;
}
button::after{
  content: none;
}
.photo{
  position: relative;
  width: 100%;
  margin: 50rpx auto;
  height: 680rpx;
  /* background: url(https://gcdn.playonwechat.com/photo/bg.png) no-repeat;
  background-size: contain; */
}
.photo image{
   position:absolute;
    opacity:1;
    -webkit-transform:scale(2);
    -ms-transform:scale(2);
    transform:scale(2);
    transition:opacity 2s,-webkit-transform 2s;
    transition:opacity 2s,transform 2s;
    transition:opacity 2s,transform 2s,-webkit-transform 2s
}
 .photo image.ball{
    opacity:1;
    -webkit-transform:scale(1);
    -ms-transform:scale(1);
    transform:scale(1);
} 
